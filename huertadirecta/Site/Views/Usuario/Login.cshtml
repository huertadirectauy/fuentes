<head>
    <meta name="viewport" content="width=device-width" />
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/Login/Login.js"></script>
    <title>Login</title>
</head>

<div class="container body-content">
    <h2>Ingresar</h2>
    </br>
    <div class="row">
        <div class="col-md-6">
            <section id="loginForm">
                <form class="form-horizontal">
                    <div>
                        <div class="form-group">
                            <label class="col-md-4 control-label" for="txtUsuario">Correo electrónico</label>
                            <div class="col-md-8">
                                <input type="text" id="txtUsuario" class="form-control" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-4 control-label" for="txtPassword">Contraseña</label>
                            <div class="col-md-8">
                                <input type="password" id="txtPassword" class="form-control" />
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-offset-10 col-md-10">
                                <input type="button" id="btnLogin" value="Login" class="btn btn-default" />
                            </div>
                        </div>
                    </div>
                </form>
            </section>
        </div>
    </div>
</div>


